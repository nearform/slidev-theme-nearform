{
  "name": "slidev-theme-nearform",
  "version": "2.0.0",
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "description": "NearForm theme for Slidev",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "slidev example.md --open"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "colorSchema": "dark",
      "fonts": {
        "mono": "PT Mono",
        "sans": "Inter",
        "serif": "Bitter"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nearform/slidev-theme-nearform"
  },
  "bugs": "https://github.com/nearform/slidev-theme-nearform",
  "dependencies": {
    "@slidev/types": "^0.48.1",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2",
    "theme-vitesse": "^0.7.2"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.0-beta.3"
  }
}
